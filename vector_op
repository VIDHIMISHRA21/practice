//vector operations

#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int> v;
    cout<<"size="<<v.size();cout<<endl;
     
    v.push_back(1);
    cout<<v.capacity();cout<<endl;
    cout<<"size="<<v.size();
    cout<<endl;


    v.push_back(2);
    cout<<v.capacity();cout<<endl;
    cout<<"size="<<v.size();
    cout<<endl;

    
     v.push_back(3);
    cout<<v.capacity();cout<<endl;
    cout<<"size="<<v.size();
    cout<<endl;

     v.push_back(4);
    cout<<v.capacity();cout<<endl;
    cout<<"size="<<v.size();
    cout<<endl;

     v.push_back(5);
    cout<<"capacity"<<v.capacity();cout<<endl;
    cout<<"size="<<v.size();
    cout<<"ele at 2"<<v.at(2);
    cout<<endl;

    cout<<"before pop"<<endl;
    for(int i:v)
    { 
        cout<<i<<" ";
        cout<<endl;
    }
    cout<<" after pop";
    v.pop_back();
    for(int i:v){
        cout<<i<<" ";
        cout<<endl;
    }
}
