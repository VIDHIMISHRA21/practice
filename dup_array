//easiest method to find duplicate

#include<bits/stdC++.h>
using namespace std;

int main(int i){
        int arr[]=  {1, 4, 4, 2, 3};
    int n =sizeof(arr)/sizeof(arr[0]);
    int m=arr[0];
    if(m==arr[i]){
    for(int i=0;i<n;i++){
        

        }
    }
        cout<<"DUPLICATE ELEMENT IS "<<m;
}
